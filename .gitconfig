[core]
	editor = nvim
	autocrlf = input
[user]
	name = krane
	email = christian.kranabetter@protonmail.com
[init]
	defaultBranch = main
[fetch]
	prune = true
[alias]
	st = status -sb
	co = checkout
	cm = commit -m
	ca = commit --amend
	ll = log --oneline
	pr = pull --rebase
	aa = add all
	dc = diff --cached

